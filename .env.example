# Example Environment Variables for AI Credit Scoring Platform
# Copy this file to .env and fill in your actual values.
# DO NOT commit the .env file to version control.

# Database (PostgreSQL Example)
POSTGRES_SERVER=localhost
POSTGRES_PORT=5432
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=credit_scoring_db

# AWS Credentials (Optional - if not using IAM roles)
# AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY_ID
# AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_ACCESS_KEY
AWS_REGION=us-east-1 # Or your preferred region
S3_BUCKET_NAME=your-unique-ai-credit-scoring-data-bucket

# Add other environment-specific variables here
# e.g., MLFLOW_TRACKING_URI=http://localhost:5000
# e.g., EXTERNAL_API_KEY=your_api_key
